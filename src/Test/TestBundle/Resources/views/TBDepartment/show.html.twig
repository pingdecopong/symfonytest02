{% extends '::base.html.twig' %}

{% block body -%}
    <h1>TBDepartment</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.Name }}</td>
            </tr>
            <tr>
                <th>Name2</th>
                <td>{{ entity.Name2 }}</td>
            </tr>
            <tr>
                <th>Sortno</th>
                <td>{{ entity.SortNo }}</td>
            </tr>
            <tr>
                <th>Deleteflug</th>
                <td>{{ entity.DeleteFlug }}</td>
            </tr>
            <tr>
                <th>Createddatetime</th>
                <td>{{ entity.CreatedDatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tbdepartment') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tbdepartment_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('tbdepartment_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
